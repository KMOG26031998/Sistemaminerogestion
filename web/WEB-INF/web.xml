<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>subirCurriculum</servlet-name>
        <servlet-class>Controller.subirCurriculum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>consultaPostulantes</servlet-name>
        <servlet-class>Controller.consultaPostulantes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BckPersonal</servlet-name>
        <servlet-class>Controller.BckPersonal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Asistencia</servlet-name>
        <servlet-class>Controller.ServAsistencia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminaAsistencia</servlet-name>
        <servlet-class>Controller.EliminaAsistencia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminaAnticipo</servlet-name>
        <servlet-class>Controller.EliminaAnticipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditaAsistencia</servlet-name>
        <servlet-class>Controller.EditaAsistencia</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>subirCurriculum</servlet-name>
        <url-pattern>/subirCurriculum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>consultaPostulantes</servlet-name>
        <url-pattern>/consultaPostulantes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BckPersonal</servlet-name>
        <url-pattern>/BckPersonal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Asistencia</servlet-name>
        <url-pattern>/ServAsistencia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminaAsistencia</servlet-name>
        <url-pattern>/EliminaAsistencia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminaAnticipo</servlet-name>
        <url-pattern>/EliminaAnticipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditaAsistencia</servlet-name>
        <url-pattern>/EditaAsistencia</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
